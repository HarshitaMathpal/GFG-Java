class Solution{
    public String removeConsecutiveCharacter(String S){
        String ans="";
   // char[] a=S.toCharArray();
        for(int i=0;i<S.length()-1;i++){
            if(S.charAt(i)!=S.charAt(i+1)){
                ans+=S.charAt(i);
            }
        }
        //if(a[a.length-1]!=a[a.length-2])
        ans+=S.charAt(S.length()-1);
        return ans;
    }
}
